{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
  "task3_api": {
    "resource_type": "api_gateway",
    "deploy_stage": "api",
    "dependencies": [],
    "resources": {
      "/hello": {
        "GET": {
          "integration": "lambda",
          "lambda_name": "hello_world",
          "request": {
            "type": "EMPTY"
          },
          "response": {
            "statusCode": 200,
            "body": {
              "message": "Hello from Lambda"
            }
          }
        }
      }
    }
  }
}
